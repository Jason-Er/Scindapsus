CREATE TABLE user (
    _id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE,
    name TEXT NOT NULL,
    password TEXT NOT NULL UNIQUE
);

select_all:
select *
from user;

select_by_name:
select *
from user
where user.name = ?;

update_user:
UPDATE user
SET name = ?, password = ?
where user._id = ?;

insert_user:
INSERT INTO user (name, password)
VALUES (?, ?);