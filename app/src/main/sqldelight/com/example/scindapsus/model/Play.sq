CREATE TABLE play (
	id INTEGER NOT NULL  PRIMARY KEY UNIQUE,
	name TEXT NOT NULL,
	extract TEXT,
	still_url TEXT
);

select_by_id:
SELECT *
FROM play
WHERE play.id = ?;

insertOnePlay:
INSERT INTO play(id, name, extract, still_url)
VALUES (?, ?, ?, ?);

updateOnePlay:
UPDATE play
SET name = ?, extract = ?, still_url = ?
WHERE id = ?;