CREATE TABLE play (
	still_url TEXT NULL ,
	extract TEXT NULL ,
	id INTEGER NOT NULL  PRIMARY KEY ,
	name TEXT NOT NULL
);

select_by_id:
select *
from play
where play.id = ?;

insertOnePlay:
INSERT INTO play(id, name, extract, still_url)
VALUES (?, ?, ?, ?);

updateOnePlay:
UPDATE play
SET name = ?, extract = ?, still_url = ?
WHERE id = ?;